{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.playlist.com/albumNew.schema.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "handle": {
      "type": "string"
    },
    "artist_id": {
      "type": "string"
    },
    "release_date": {
      "type": "string"
    },
    "image": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["id", "name", "artist_id", "release_date", "image"]
}
