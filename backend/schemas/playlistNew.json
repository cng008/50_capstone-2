{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.playlist.com/playlistNew.schema.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "handle": {
      "type": "string"
    },
    "user_id": {
      "type": "integer"
    },
    "description": {
      "type": "string"
    },
    "created_at": {
      "type": "string"
    },
    "image": {
      "type": "string",
      "format": "uri"
    }
  },
  "additionalProperties": false,
  "required": ["name", "user_id", "image"]
}
